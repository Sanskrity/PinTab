{
    "manifest_version": 2,
    "name": "PinTab",
    "description": "Pin your favourite tabs for quick access without cluttering your Chrome titlebar",
    "version": "1.0",
     "icons": {"128": "icon_128.png"},
     "browser_action": {
         "default_icon": "icon.png",
         "default_popup": "popup.html"
     },

     "content_security_policy":"script-src 'self' https://apis.google.com; https://ssl.google-analytics.com; object-src 'self'",
     "permissions": [
         "activeTab", 
         "storage",
         "tabs"
        ]

}